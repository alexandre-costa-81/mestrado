@article{Dekker2009,
abstract = {The monitoring and support of university freshmen is considered very important at many educational institutions. In this paper we describe the results of the educational data mining case study aimed at predicting the Electrical Engineering (EE) students drop out after the first semester of their studies or even before they enter the study program as well as identifying success-factors specific to the EE program. Our experimental results show that rather simple and intuitive classifiers (decision trees) give a useful result with accuracies between 75 and 80{\%}. Besides, we demonstrate the usefulness of cost-sensitive learning and thorough analysis of misclassifications, and show a few ways of further prediction improvement without having to collect additional data about the students.},
annote = {Este artigo tem um explica{\c{c}}{\~{a}}o bem avan{\c{c}}ada sobre a execu{\c{c}}{\~{a}}o, configura{\c{c}}{\~{a}}o dos algoritmos e sele{\c{c}}{\~{a}}o de atributos. Um dos melhores artigos.},
author = {Dekker, Gerben W and Pechenizkiy, Mykola and Vleeshouwers, Jan M},
file = {:home/alexandre/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Dekker, Pechenizkiy, Vleeshouwers - 2009 - Predicting Students Drop Out A Case Study.pdf:pdf},
journal = {the 2nd International Conference on Educational Data Mining},
mendeley-groups = {Minera{\c{c}}{\~{a}}o de Dados Educacionais/N{\~{a}}o classificados,Mestrado/Artigos Relacionados},
pages = {41--50},
title = {{Predicting Students Drop Out: A Case Study}},
url = {http://www.win.tue.nl/{~}mpechen/research/edu.html.},
year = {2009}
}

@article{Manhaes2011,
abstract = {As universidades enfrentam o desafio de reduzir os {\'{i}}ndices de evas{\~{a}}o dos alunos nos cursos de gradua{\c{c}}{\~{a}}o. O problema ocorre em diversas universidades brasileiras gerando preju{\'{i}}zos para o pa{\'{i}}s, alunos e universidades. Neste sentido, nosso objetivo {\'{e}} identificar precocemente alunos em risco de evas{\~{a}}o nos cursos de gradua{\c{c}}{\~{a}}o. A t{\'{e}}cnica de minera{\c{c}}{\~{a}}o de dados oferece diversos algoritmos que podem ser empregados para identificar alunos em risco de abandono. Este trabalho apresenta um estudo utilizando dados acad{\^{e}}micos de alunos de gradua{\c{c}}{\~{a}}o de uma universidade brasileira- UFRJ. Os resultados mostraram que utilizando as primeiras notas semestrais dos calouros {\'{e}} poss{\'{i}}vel identificar com precis{\~{a}}o de 80{\%} a situa{\c{c}}{\~{a}}o final do aluno no curso.},
author = {Manh{\~{a}}es, Laci Mary Barbosa and da Cruz, S{\'{e}}rgio Manuel Serra and Costa, Raimundo J. Macario and Zavaleta, Jorge and Zimbr{\~{a}}o, Geraldo and da Cruz, S{\'{e}}rgio Manuel Serra and Costa, Raimundo J. Macario and Zavaleta, Jorge and Zimbr{\~{a}}o, Geraldo},
doi = {10.5753/CBIE.SBIE.2011.%P},
